{
  "name": "媒体消息测试-media_key类型错误",
  "description": "测试发送时 media_key 使用非字符串类型（如 int、bool、null）",
  "category": "negative",
  "request": {
    "method": "POST",
    "endpoint": "/open-apis/im/v1/messages",
    "query_params": {
      "receive_id_type": "open_id"
    },
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer $access_token$"
    },
    "body": {
      "receive_id": "$open_id$",
      "msg_type": "media",
      "content": {
        "__needs_json_encode": true,
        "file_key": 12345,
        "image_key": true
      },
      "uuid": "$random_uuid$"
    }
  },
  "expected": {
    "status_code": 400,
    "headers": {
      "Content-Type": "application/json"
    },
"body": {
      "code": {
        "@not_equal": 0
      },
      "msg": {
          "@not_empty": true
        }
    }
  },
  "setup": [
    {
      "action": "get_tenant_access_token",
      "save_to": "access_token"
    },
    {
      "action": "get_open_id",
      "save_to": "open_id"
    },
    {
      "action": "generate_uuid",
      "save_to": "random_uuid"
    }
  ]
}
