{
  "name": "卡片空 config 测试",
  "description": "不设置 config 字段，验证卡片是否可以正常发送",
  "category": "negative",
  "request": {
    "method": "POST",
    "endpoint": "/open-apis/im/v1/messages",
    "query_params": {
      "receive_id_type": "open_id"
    },
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer $access_token$"
    },
    "body": {
      "receive_id": "$open_id$",
      "msg_type": "interactive",
      "content": {
        "__needs_json_encode": true,
        "schema": "2.0",
        "header": {
          "title": {
            "tag": "plain_text",
            "content": "不设置 config 测试"
          }
        },
        "elements": [
          {
            "tag": "div",
            "text": {
              "tag": "plain_text",
              "content": "测试不设置 config"
            }
          }
        ]
      },
      "uuid": "$random_uuid$"
    }
  },
  "expected": {
    "status_code": 400,
"body": {
      "code": {
        "@not_equal": 0
      },
      "msg": {
          "@not_empty": true
        }
    }
  },
  "setup": [
    {
      "action": "get_tenant_access_token",
      "save_to": "access_token"
    },
    {
      "action": "get_open_id",
      "save_to": "open_id"
    },
    {
      "action": "generate_uuid",
      "save_to": "random_uuid"
    }
  ]
}