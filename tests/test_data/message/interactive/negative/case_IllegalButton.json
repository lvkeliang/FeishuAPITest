{
  "name": "异常_非法 button.type",
  "description": "button 使用未定义的 type 类型（jump）",
  "category": "negative",
  "request": {
    "method": "POST",
    "endpoint": "/open-apis/im/v1/messages",
    "query_params": {
      "receive_id_type": "open_id"
    },
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer $access_token$"
    },
    "body": {
      "receive_id": "$open_id$",
      "msg_type": "interactive",
      "content_json": {
        "type": "message",
        "elements": [
          {
            "tag": "button",
            "text": {
              "tag": "plain_text",
              "content": "点击我"
            },
            "type": "jump"
          }
        ]
      },
      "uuid": "$random_uuid$"
    }
  },
  "expected": {
    "status_code": 400,
"body": {
      "code": {
        "@not_equal": 0
      },
      "msg": {
          "@not_empty": true
        }
    }
  }
}